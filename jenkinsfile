node{
  stage('clone'){
    git 'https://github.com/PrakashGit404/VProfile.git'
  }
  stage('compile-pack'){
    def mvnHome = tool name: 'mvn', type: 'maven'
    sh "${mvnHome}/bin/mvn clean compile package install"
  }
  stage('sonar qube'){
    def mvnHome = tool name: 'mvn', type: 'maven'
      withSonarQubeEnv('sonarqube'){
      sh "${mvnHome}/bin/mvn sonar:sonar"
    }
  }
  // stage('checkstyle install'){
  //  def mvnHome = tool name: 'mvn', type: 'maven'
  //  sh "${mvnHome}/bin/mvn install checkstyle:checkstyle"
 // }
    stage('Publish') {
     nexusArtifactUploader artifacts: [[artifactId: 'vprofile', classifier: '', file: 'target/*.war', type: 'war']], credentialsId: 'Nexus3.14', groupId: 'com.visualpathit', nexusUrl: '10.160.0.7:8081', nexusVersion: 'nexus2', protocol: 'http', repository: 'maven-releases', version: '${BUILD_ID}'
    }
}
